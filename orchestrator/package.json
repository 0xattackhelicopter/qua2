  "description": "Deployment orchestrator with support for multiple providers",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx tsc",
    "serve": "node dist/src/server.js",
    "proxy": "node dist/src/core/providers/spheron/proxy.js",
